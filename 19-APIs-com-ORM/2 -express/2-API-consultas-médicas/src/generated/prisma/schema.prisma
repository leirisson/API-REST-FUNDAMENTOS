generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Medico {
  id            String @id @default(uuid())
  nome          String
  especialidade String
  crm           String @unique

  @@map("medicos")
}
