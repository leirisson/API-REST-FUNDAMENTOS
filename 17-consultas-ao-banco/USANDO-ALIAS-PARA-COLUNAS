-- FORMA DE USAR
SELECT COUNT(*) AS TOTAL FROM produtos 

-- FORMA DE USAR 2
SELECT COUNT(*) TOTAL FROM produtos

-- FORMA DE USAR 3
SELECT COUNT(*) 'TOTAL' FROM produtos

-- FORMA DE USAR 4
SELECT COUNT(*) AS 'TOTAL DE PRODUTOS' FROM produtos

-- FORMA DE USAR 5
SELECT COUNT(*) AS [TOTAL DE PRODUTOS] FROM produtos


SELECT id AS 'codigo produto', nome AS [NOME CLIENTE], preco  FROM produtos